{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/join-waitlist",
      "dest": "server.js"
    },
    {
      "src": "/(?!join-waitlist)(.*)",
      "dest": "/$1"
    },
    {
      "src": "/",
      "dest": "index.html"
    }
  ]
}
